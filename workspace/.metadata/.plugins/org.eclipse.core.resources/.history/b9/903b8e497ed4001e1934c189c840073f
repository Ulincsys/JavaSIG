package edu.missouri.mca.javasig;

public abstract class MyAbstract {
	private int x = 0;
	
	public MyAbstract() {
		this.x = 0;
	}
	
	public MyAbstract(int x) {
		this.x = x;
	}
	
	public void fun() {
		System.out.println("abstract public impl");
	}
	
	private void fun2() {
		System.out.println("abstract private impl");
	}
	
	// Cannot be private
	abstract void fun3();
}
